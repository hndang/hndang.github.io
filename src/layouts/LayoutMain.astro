---
import Layout from "./Layout.astro";
import Sidebar from "../components/Sidebar.astro";
import Footer from "../components/Footer.astro";
interface Props {
    title: string;
}
const { title } = Astro.props;
---

<Layout title=`${title}`>
    <Sidebar slot="header"/>
    <slot/>
    <Footer/>
    <div class="h-[20vh]" />
</Layout>